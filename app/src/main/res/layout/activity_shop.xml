<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.shop.ShopActivity">
        <RelativeLayout
            android:id="@+id/page_shops_select_ek1"
            android:layout_marginTop="0dp"
            android:layout_marginStart="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp">

            <View
                android:id="@+id/_bg__shops_select_ek2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/_bg__shops_select_ek2_shape" />

            <View
                android:id="@+id/background"
                android:layout_width="match_parent"
                android:layout_height="185dp"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/background_shape" />


            <RelativeLayout
                android:id="@+id/shops_selection"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="300dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp">


                <GridLayout
                    android:id="@+id/shops_selection_box"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp">

                    <GridView
                        android:id="@+id/gridview"
                        android:layout_width="390dp"
                        android:layout_height="match_parent"
                        android:numColumns="2"
                        android:horizontalSpacing="4dp"
                        android:verticalSpacing="4dp" />
                </GridLayout>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/shape_ek4"
                android:layout_marginTop="-109dp"
                android:layout_marginStart="-99dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-99dp">

                <View
                    android:id="@+id/ellipse_1_ek4"
                    android:background="@drawable/ellipse_1_ek4_shape"
                    android:alpha="0.49000000953674"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="70dp"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="0dp" />

                <View
                    android:id="@+id/ellipse_2_ek4"
                    android:background="@drawable/ellipse_2_ek4_shape"
                    android:alpha="0.49000000953674"
                    android:layout_marginStart="90dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="90dp" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/search_ek1"
                android:layout_marginTop="200dp"
                android:layout_marginStart="25dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp">

                <View
                    android:id="@+id/rectangle_2_ek7"
                    android:background="@drawable/rectangle_2_ek7_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="325dp"
                    android:layout_height="51dp"
                    android:layout_marginLeft="0dp" />

                <TextView
                    android:id="@+id/search_ek2"
                    android:textSize="13sp"
                    android:textColor="@color/welcome_back__color"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="16dp"
                    android:layout_width="178dp"
                    android:layout_height="27.5dp"
                    android:text="@string/search_ek2_string" />

            </RelativeLayout>


            <RelativeLayout android:id="@+id/desc_and_back"
                android:layout_marginTop="48dp"
                android:layout_marginStart="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp">

                <View android:id="@+id/desc_and_back_ek1"
                    android:background="@drawable/desc_and_back_ek1_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="342dp"
                    android:layout_height="35dp"
                    tools:ignore="RtlCompat" />

                <Button
                    android:id="@+id/buttonBackShop"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginBottom="-50dp"
                    android:background="@drawable/_back" />

                <TextView
                    android:id="@+id/laundry_shop_delivery"
                    android:textStyle="bold"
                    android:textColor="@color/welcome_back__color"
                    android:layout_marginLeft="44dp"
                    android:layout_marginTop="3dp"
                    android:layout_width="179dp"
                    android:layout_height="25dp"
                    android:text="@string/laundry_shop_delivery_string" />

                <TextView
                    android:id="@+id/home"
                    android:textColor="@color/welcome_back__color"
                    android:layout_marginLeft="44dp"
                    android:layout_marginTop="17dp"
                    android:layout_width="50dp"
                    android:layout_height="19dp"
                    android:text="@string/home_string" />


            </RelativeLayout>
            <ProgressBar
                android:id="@+id/progressBarLoadStore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="32dp"
                android:layout_marginTop="64dp"
                android:layout_marginEnd="32dp"
                android:layout_marginBottom="64dp"
                android:visibility="gone" />
        </RelativeLayout>
</RelativeLayout>